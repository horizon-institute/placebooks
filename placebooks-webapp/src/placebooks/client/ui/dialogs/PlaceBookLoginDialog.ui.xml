<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p="urn:import:placebooks.client.ui.elements" xmlns:f="urn:import:placebooks.client.ui.forms" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="en">
	<ui:style>
		.button {
			margin: 10px 0 0 0;
		}
		
		.buttonPanel {
			display: -moz-box;
			display: -webkit-box;
			-moz-box-pack: end;
			-webkit-box-pack: end;
		}
	</ui:style>
	<ui:with field='images' type='placebooks.client.ui.images.Images' />	
	<g:FlowPanel>
		<g:Label ui:field="usernameLabel"><ui:msg>UserName</ui:msg>:</g:Label>
		<g:TextBox ui:field="username" name="pb_username" />
		<g:Label><ui:msg>Password</ui:msg>:</g:Label>
		<g:PasswordTextBox ui:field="password" name="pb_password" />
		<g:SimplePanel styleName="{style.buttonPanel}"><g:Button styleName="{style.button}" ui:field="submit">Login</g:Button></g:SimplePanel>
	</g:FlowPanel>
</ui:UiBinder> 